# Development Leads Finder - Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# SerpAPI Configuration (for web search)
SERPAPI_API_KEY=your_serpapi_key_here

# Google Sheets & Drive API Configuration
GOOGLE_CREDENTIALS_PATH=./google_credentials.json

# Logging Configuration
LOG_LEVEL=INFO

# Pipeline Configuration
MIN_DEV_SCORE=50.0
SEARCH_LOCATION=Newton, MA

# Database Configuration (for Task 2)
DATABASE_URL=sqlite:///./development_leads.db

# Email Configuration (for Task 3)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SENDER_EMAIL=your_email@gmail.com
SENDER_PASSWORD=your_app_password

# Slack Configuration (for Task 3)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# Google Maps API (for Task 4 - Maps)
GOOGLE_MAPS_API_KEY=your_google_maps_key_here
